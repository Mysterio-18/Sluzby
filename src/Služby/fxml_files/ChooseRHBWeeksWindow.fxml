<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Služby.PopUpWindowsController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="VYBER">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Text>
      <CheckBox mnemonicParsing="false" text="1. týden" fx:id="RHB_week_one">
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </CheckBox>
      <CheckBox mnemonicParsing="false" text="2. týden" fx:id="RHB_week_two"/>
      <CheckBox mnemonicParsing="false" text="3. týden" fx:id="RHB_week_three">
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </CheckBox>
      <Button mnemonicParsing="false" text="Potvrdit" onAction="#RHB_weeks_confirm"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
