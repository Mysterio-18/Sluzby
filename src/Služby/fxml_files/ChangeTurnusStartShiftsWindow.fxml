<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="25.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="Služby.PopUpWindowsController">
    <padding>
        <Insets bottom="10.0" top="10.0"/>
    </padding>
    <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="400.0"
           text="Chcete změnit začátek turnusu a výběr směn?">
        <padding>
          <Insets top="10.0"/>
        </padding>
    </Label>
    <HBox alignment="CENTER" spacing="30.0">
        <padding>
            <Insets bottom="10.0"/>
        </padding>
        <Button mnemonicParsing="false" prefWidth="100.0" text="Ano" onAction="#changeTurnusYes">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" prefWidth="100.0" text="Ne" onAction="#changeTurnusNo"/>
    </HBox>
</VBox>
