<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="Služby.PopUpWindowsController">
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Napište den začátku turnusu"/>
    <TextField promptText="0" fx:id="arrival_date"/>
    <Button mnemonicParsing="false" text="Potvrdit" onAction="#arrival_date_confirm"/>
</VBox>
